{
	"statements":[
		{
			"statement":"CREATE (b1:Book {title:{title},tags:{tags}})<-[a:AuthorOf]-(p:Person{name:{name}}) RETURN b1,a,p",
			"parameters":{
				"title":"Things Fall Apart",
				"tags":[
					"novel"
				],
				"name":"Fyodor Dostoevsky"
			}
		},
		{
			"statement":"CREATE (b1) <-[v:Votes{score: round(rand()*3) + 2 }]- (u:User{name:{username}}) RETURN b1,v,u",
			"parameters":{
				"username":"dav"
			}
		},
		{
			"statement":"CREATE (b:Book {title:{title},tags:{tags}})<-[a:AuthorOf]-(p:Person{name:{name}}) RETURN b,a,p",
			"parameters":{
				"title":"Pride and Prejudice",
				"tags":[
					"novel"
				],
				"name":"Jane Austen"
			}
		},
		{
			"statement":"MATCH (b:Book {title:{title},tags:{tags}}) WITH b CREATE (b) <-[v:Votes{score: round(rand()*3) + 2 }]- (u:User{name:{username}}) RETURN b,v,u",
			"parameters":{
				"title":"Pride and Prejudice",
				"tags":[
					"novel"
				],
				"username":"mike"
			}
		}
	]
}